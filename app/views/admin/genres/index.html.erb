 <%= flash[:notice] %>

  <h1>ジャンル一覧・追加</h1>

  <h2>ジャンル名</h2>
    <%= form_with model: @genre, url: admin_genres_path do |f| %>
    <%= f.text_field :name %>
    <%= f.submit "新規登録"%>
    <% end %>

  <div>
    <table>
      <th>ジャンル</th>
        <tbody>
          <tr class="item-box">
              <% @genres.each do |genre| %></tr>
                <td>
                  <%= genre.name %>
                    <%= link_to "編集",edit_admin_genre_path(genre.id) %>
                </td>
              <% end %>
          </tbody>
        </table>
  </div>