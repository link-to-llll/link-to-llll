<h1>管理者注文履歴一覧</h1>

  <table class='table table-hover table-inverse'>
    <thead>
      <tr>
        <th>宛名</th>
        <th>ステータス</th>
      </tr>
    </thead>
    <tbody>
     <% @orders.each do |order| %>
      <tr>
        <td><%= order.shipping_name %></td>
        <td><%= order.status %></td>
        <td><%= link_to admin_order_path(order) do %>
          <%= order.updated_at %>
        </td>
          <% end %>
      </tr>
    <% end %>
  </tbody>
</table>


