<h1>管理者注文履歴一覧</h1>

  <table class='table table-hover table-inverse'>
    <thead>
      <tr>
        <th>注文日時</th>
        <th>購入者</th>
        <th>注文個数</th>
        <th>注文ステータス</th>
      </tr>
    </thead>
    <tbody>
     <% @orders.each do |order| %>
      <tr>
         <td><%= link_to admin_orders_path(order) do %>
          <%= order.updated_at %>
           <% end %>
        <td><%= order.shipping_name %></td>
      　<td><%= order.order_products %></td>
        <td><%= order.status %></td>
        </td>

      </tr>
    <% end %>
  </tbody>
</table>


