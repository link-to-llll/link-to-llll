 <h2>配送先編集画面</h2>

<%= form_with model:@shipping_address do |f| %>
  <div class="form-group">
     <%= f.label :宛名 %><br>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
     <%= f.label :配送先郵便番号 %><br>
    <%= f.text_field :post_code %>
  </div>
  <div class="form-group">
     <%= f.label :配送先住所 %><br>
    <%= f.text_field :address %>
  </div>
  <div class="form-group">
    <%= f.submit '更新' %>
  </div>
<% end %>